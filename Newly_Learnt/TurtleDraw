import turtle

def draw():
    x=turtle.Turtle()
    x.penup()
    x.goto(-150,-150)
    x.pendown()
    x.left(90)
    for i in range(2):
        for j in range(4):
            x.right(90)
            x.pendown()
            x.forward(300)
            x.right(180)
            x.forward(300)
            x.right(90)
            x.penup()
            x.forward(100)
        x.backward(100)
        x.right(90)
